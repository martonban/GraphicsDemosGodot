code = "shader_type canvas_item;

uniform float timeFactor = 0.2f;
uniform vec2 amplitude = vec2(10.0f, 5.0f);

void vertex() {
	VERTEX.x += cos(TIME * timeFactor + VERTEX.x + VERTEX.y) * amplitude.x;
	VERTEX.y += sin(TIME * timeFactor + VERTEX.y + VERTEX.x) * amplitude.y;

}"

